- if flash[:error]
  %div.alert.alert-danger
    = flash[:error]

- unless @submissions.nil?
  %div#filtered-results
    %ul
      = will_paginate @submissions, renderer: BootstrapPagination::Rails
      = render partial: "form_submissions/form_submission", collection: @submissions
      = will_paginate @submissions, renderer: BootstrapPagination::Rails
